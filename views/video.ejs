<header>
    <nav>
        <a class="navbar-brand" href="/">
            <img src="logo.png" width="25%">
        </a>
    </nav>
</header>
<div id="videocontainer"></div>
<a href="/" id="b" class="btn btn-light">Download Video</a>
<script>
    var vars = {};
    var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function (m, key, value) {
        vars[key] = value.replace(/%20/g, " ");
    });
    console.log(window.location.href)
    var video = document.createElement('video');
    video.width = "650";
    video.height = "650"
    video.muted = true;
    video.controls = true;
    var container = document.getElementById('videocontainer');
    container.appendChild(video);
    var source = document.createElement('source');
    source.src = 'video?storage=' + vars["storage"] + '&video=' + vars["video"]+vars["ext"];
    source.type = "video/mp4";

    video.appendChild(source);
    console.log(vars["video"]);

    video.play();
    function getVideo() {
        console.log(vars["video"]);
    }
    document.getElementById('b').href = 'download?storage='+ vars["storage"] +'&video=' + vars["video"]+vars["ext"];

</script>